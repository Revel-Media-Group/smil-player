<smil>
    <head>
        <meta http-equiv="Refresh" content="999999"/>
        <layout>
            <root-layout width="1920" height="1080" background="#000000"/>
            <region width="1920" height="1080" backgroundColor="#000000" regionName="main" left="0" top="0" z-index="1"
                    mediaAlign="center"/>
            <transition id="fade3s" xml:id="fade3s" type="fade" subtype="crossfade" dur="3s"/>
        </layout>
    </head>
    <body>
        <par>
            <seq>
                <prefetch
                        src="https://res.cloudinary.com/designage/image/upload/q_auto,f_auto,w_1920,h_1080,/v1625810684/PROFILE/110e1b6c-dc6d-4ce2-8b6e-a31efc86b268/jf0fkuwcog0hbf9qxdvw.jpg"/>
                <prefetch
                        src="https://res.cloudinary.com/designage/image/upload/q_auto,f_auto,w_1920,h_1080,/v1625810685/PROFILE/110e1b6c-dc6d-4ce2-8b6e-a31efc86b268/lq9kiz18clo9w1fl5jlz.png"/>
                <seq id="__prefetchEnd" dur="1s"/>
            </seq>
            <par begin="__prefetchEnd.endEvent" repeatCount="indefinite">
                <seq expr="compare(time(), '00:00:00') >= 0 and [compare(time(), '23:59:59') <= 0 or [compare(time(), '00:00:00') >= 0 and compare(time(), '00:00:00') < 0]]">
                    <img region="main" dur="5s" transIn="fade3s"
                         src="https://res.cloudinary.com/designage/image/upload/q_auto,f_auto,w_1920,h_1080,/v1625810684/PROFILE/110e1b6c-dc6d-4ce2-8b6e-a31efc86b268/jf0fkuwcog0hbf9qxdvw.jpg"
                         fit="meetBest"/>
                    <img region="main" dur="5s"
                         src="https://res.cloudinary.com/designage/image/upload/q_auto,f_auto,w_1920,h_1080,/v1625810685/PROFILE/110e1b6c-dc6d-4ce2-8b6e-a31efc86b268/lq9kiz18clo9w1fl5jlz.png"
                         fit="meetBest"/>

                    <ref src="https://signageos-demo.s3.eu-central-1.amazonaws.com/smil/zones/files/bottomWidget.wgt" type="application/widget" region="main" dur="5s">
                        <param name="cacheControl" value="onlyIfCached" />
                    </ref>

                    <video src="https://static.signageos.io/assets/video-test-1_e07fc21a7a72e3d33478243bd75d7743.mp4" region="main" soundLevel="0%"></video>
                </seq>
            </par>
        </par>
    </body>
</smil>

